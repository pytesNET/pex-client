{
    "name": "@pytes.net/pex.js",
    "version": "0.1.0",
    "description": "Minimalistic JavaScript client layer for the PEX - Printer Execution python server.",
    "license": "MIT",
    "author": "Sam",
    "type": "module",
    "main": "./dist/pex.cjs",
    "module": "./dist/pex.mjs",
    "types": "./dist/pex.d.ts",
    "unpkg": "./dist/pex.js",
    "jsdelivr": "./dist/pex.js",
    "exports": {
        ".": {
            "types": "./dist/pex.d.ts",
            "import": "./dist/pex.mjs",
            "require": "./dist/pex.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rimraf dist && rollup -c",
        "dev": "rollup -c -w",
        "typecheck": "tsc --noEmit",
        "prepublishOnly": "npm run build && npm run typecheck"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^29.0.0",
        "@rollup/plugin-node-resolve": "^16.0.3",
        "@rollup/plugin-terser": "0.4.4",
        "@rollup/plugin-typescript": "^12.3.0",
        "@types/node": "22.13.14",
        "rollup": "^4.53.2",
        "rollup-plugin-dts": "^6.2.3",
        "rimraf": "^6.1.0",
        "tslib": "2.8.1",
        "typescript": "^5.9.3"
    }
}